package semester1;

public class ArrayOrNot {
	public static void main(String[] args) {

		int[] arr = { 12, 23, 34, 45, 56, 67, 78, 89, 90 };
		int target = 4;
		boolean flag = false;
		for (int i = 0; i < arr.length; i++) {
			if (arr[i] == target) {
				flag = true;
				break;

			}
		}
		if (flag == true) {
			if (flag == true)
				System.out.println("Target exists in array");
		} else
			System.out.println("Target is missing in array");
	}
}
